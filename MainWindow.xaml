                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="200"/>
                                <ColumnDefinition Width="200"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="20"/>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Image Grid.Column="0" Grid.RowSpan="3" Source="https://www.coca-cola.com/content/dam/brands/tw/coca-cola/image/coke-logo.png" Width="100"  HorizontalAlignment="Left"/>
                            <Label Content="Coca-Cola CORP" Foreground="White" Grid.Column="1" Grid.Row="1" FontSize="23" HorizontalContentAlignment="Center"/>
                            <Label x:Name="lable_ColaPrice" Grid.Column="2" Grid.Row="1" HorizontalContentAlignment="Center" Foreground="Green"/>
                            <Label x:Name="lable_ColaProcent" Grid.Column="3" Grid.Row="1" HorizontalContentAlignment="Center" Foreground="Green"/>
                            <Grid Grid.ColumnSpan="3" Grid.Column="1" Grid.Row="0" Background="Gray"/>
