                            <Label Content="Google CORP" Foreground="White" Grid.Column="1" Grid.Row="1" FontSize="23" HorizontalContentAlignment="Center"/>
                            <Label x:Name="lable_GooglePrice" Grid.Column="2" Grid.Row="1" HorizontalContentAlignment="Center" Foreground="Green"/>
                            <Label x:Name="lable_GoogleProcent" Grid.Column="3" Grid.Row="1" HorizontalContentAlignment="Center" Foreground="Green"/>
                            <Grid Grid.ColumnSpan="3" Grid.Column="1" Grid.Row="0" Background="Gray"/>
                            <Grid Grid.ColumnSpan="3" Grid.Column="1" Grid.Row="2" Background="Gray"/>
                        </Grid>
                    </Expander>
                    <Expander Header="Coca-Cola" IsExpanded="True" FontSize="23" Foreground="White">
